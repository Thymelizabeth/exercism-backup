exercism.rotationalCipher.rotate : Text -> Nat -> Text
exercism.rotationalCipher.rotate text shiftKey =
			shiftKey' = mod shiftKey 26
			map (cases
			 c
			   | not <| isLetter c -> c
			   | isUpper c && getOrElse ?훮 (Char.fromNat (Char.toNat c + shiftKey')) > ?Z || isLower c && getOrElse ?훮 (Char.fromNat (toNat c + shiftKey')) > ?z -> getOrElse ?훮 <| Char.fromNat (toNat c + shiftKey' - 26)
			   | otherwise -> getOrElse ?훮 <| Char.fromNat (toNat c + shiftKey')
			 ) text
